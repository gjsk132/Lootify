<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SmallButton">
        <attr name="text" format="string"/>
        <attr name="text_color" format="color"/>
        <attr name="enable" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="DialogBase">
        <attr name="sub_title" format="string"/>
        <attr name="description" format="string"/>
        <attr name="add_icon_visible" format="boolean"/>
        <attr name="is_buttons" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CreateMapSettingButton">
        <attr name="main_text" format="string"/>
        <attr name="sub_text" format="string"/>
        <attr name="setting_cnt" format="string"/>
    </declare-styleable>

    <declare-styleable name="InputField">
        <attr name="question" format="string"/>
        <attr name="is_required" format="boolean"/>
        <attr name="hint" format="string"/>
    </declare-styleable>
</resources>